# GitLab instance URL
GITLAB_URL=https://gitlab.com

# Output directory
OUTPUT_DIR=./gitlab-repos

# Use SSH for cloning
USE_SSH=false

# Number of parallel workers
MAX_WORKERS=5

# Retry attempts for failed clones
MAX_RETRIES=3

# Delay between retries
RETRY_DELAY_SECONDS=2

# Clone specific group only
GROUP_ID=

# GitLab personal access token
GITLAB_TOKEN=
